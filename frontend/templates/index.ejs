    <script>
    $(document).ready(function() {
        // Sticky navbar
        $(window).scroll(function() {
            if($(this).scrollTop() > 100) {
                $('.navbar-area').addClass('sticky');
            } else {
                $('.navbar-area').removeClass('sticky');
            }
            
            // Back to top button
            if ($(this).scrollTop() > 300) {
                $('.back-to-top').addClass('active');
            } else {
                $('.back-to-top').removeClass('active');
            }
        });
        
        // Smooth hover effects on feature boxes
        $('.single-about').hover(function() {
            $(this).find('.about-icon img').css('transform', 'scale(1.1)');
        }, function() {
            $(this).find('.about-icon img').css('transform', 'scale(1)');
        });
        
        // Auto slide carousel with pause on hover
        $('#carouselOne').carousel({
            interval: 5000,
            pause: "hover"
        });
        
        // Animation for CTA buttons
        $('.cta-button').hover(function() {
            $(this).css('color', '#FFAD00');
        }, function() {
            $(this).css('color', 'white');
        });
        
        // Interactive team member cards
        $('.team-member').hover(function() {
            $(this).find('.team-img').css('transform', 'scale(1.05)');
            $(this).find('.team-social').css('opacity', '1');
        }, function() {
            $(this).find('.team-img').css('transform', 'scale(1)');
            $(this).find('.team-social').css('opacity', '0');
        });
        
        // Back to top button
        $('.back-to-top').click(function(e) {
            e.preventDefault();
            $('html, body').animate({scrollTop: 0}, '300');
        });
        
        // Counter animation
        $('.count').each(function () {
            $(this).prop('Counter', 0).animate({
                Counter: $(this).text()
            }, {
                duration: 2000,
                easing: 'swing',
                step: function (now) {
                    $(this).text(Math.ceil(now));
                }
            });
        });
        
        // Form submission
        $('#contactForm').submit(function(e) {
            e.preventDefault();
            alert('Thank you for your message! We will get back to you soon.');
            this.reset();
        });
        
        // Initialize WOW.js for animations
        new WOW().init();
        
