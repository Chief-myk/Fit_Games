            position: relative;
            display: inline-block;
            margin-bottom: 20px;
            color: var(--dark-color);
        }
        
        .section-title .title::after {
            content: '';
            position: absolute;
            bottom: -10px;
            left: 50%;
            transform: translateX(-50%);
            width: 80px;
            height: 3px;
            background: linear-gradient(to right, var(--primary-color), var(--secondary-color));
        }
        
        .about-icon img {
            height: 60px;
            margin-bottom: 20px;
        }
        
        .about-title {
            font-weight: 700;
            margin-bottom: 15px;
            color: var(--dark-color);
        }
        
        /* Feature Counter */
        .counter-section {
            background: linear-gradient(135deg, var(--dark-color), #1a1a1a);
            padding: 80px 0;
            color: white;
        }
        
        .counter-box {
